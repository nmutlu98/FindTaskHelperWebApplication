                                        <option>Grocery Shopping</option>
                                        <option>Software</option>
                                        <option>Web Design</option>
                                        <option>Teaching</option>
                                        <option>Baby Sitting</option>
                                        <option>Shipping</option>
                                        <option>Plumbing</option>
                                        <option>Dog Walking</option>
                                        <option>Gardening</option>
                                        <option>Other</option>
                                    </select>
                                </div>
                            </div>
                            <div class="row form-group mb-4">
                                <div class="col-md-12 mb-3 mb-md-0">
                                    <label asp-for="PaymentAmountField" class="text-black" for="fname">Payment Amount</label>
                                    <input required asp-for="PaymentAmountField" type="text" id="payment-amount" class="form-control">
                                </div>
                            </div>
                            <div class="row form-group mb-4">
                                <div class="col-md-12 mb-3 mb-md-0">
                                    <label asp-for="Currency" class="text-black" for="fname">Payment Currency</label>
